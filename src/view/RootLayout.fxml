<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" focusTraversable="true" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="service.RootLayoutController">
    <top>
        <ButtonBar blendMode="DARKEN" nodeOrientation="RIGHT_TO_LEFT" prefHeight="59.0" prefWidth="580.0"
                   BorderPane.alignment="CENTER">
            <buttons>
                <Button fx:id="newEventWindowButton" minWidth="41.0" mnemonicParsing="false"
                        onAction="#handleNewEventButton" prefHeight="25.0" prefWidth="104.0" text="+"/>
            </buttons>
            <BorderPane.margin>
                <Insets left="20.0" top="5.0"/>
            </BorderPane.margin>
            <cursor>
                <Cursor fx:constant="DEFAULT"/>
            </cursor>
        </ButtonBar>
    </top>
</BorderPane>
